<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <description>asonasの個人サイト</description>
    <link>https://www.ason.as</link>
    <atom:link href="https://www.ason.as/feed.xml" rel="self" />
    <title>ason.as</title>
    <managingEditor>iam@ason.as</managingEditor>
    <% @articles.each do |article| %>
    <item>
      <title><![CDATA[<%= article.title %>]]></title>
      <description><![CDATA[<%= article.summary %>]]></description>
      <link><%= article.url %></link>
      <pubDate><%= article.published_at %></pubDate>
      <content:encoded><![CDATA[<%= article.rendered_body %>]]></content:encoded>
    </item>
    <% end %>
  </channel>
</rss>
